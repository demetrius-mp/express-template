{
  "name": "express-template",
  "version": "0.0.1",
  "description": "Express template using TypeScript, and ESLint.",
  "main": "index.js",
  "scripts": {
    "dev": "tsnd --transpile-only --exit-child --respawn --experimental-specifier-resolution=node --ignore-watch node_modules --watch src src/index.ts",
    "clean": "rimraf build/",
    "prebuild": "npm run clean",
    "build": "tsc && tsc-alias",
    "start": "node build/src/index.js",
    "lint": "eslint \"src/**\"",
    "lint:fix": "eslint --fix \"src/**\"",
    "docker": "docker compose --env-file .env.docker up"
  },
  "prisma": {
    "seed": "ts-node src/lib/databaseSeed.ts"
  },
  "author": "Demetrius Panovitch <demetrius.mp789@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsc-alias": "^1.6.7",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4",
    "typescript-transform-paths": "^3.3.1"
  },
  "dependencies": {
    "@prisma/client": "^3.11.1",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.14.0",
    "jsonwebtoken": "^8.5.1",
    "prisma": "^3.11.1",
    "prisma-exclude": "^1.0.2"
  },
  "engines": {
    "node": "16.15.0",
    "npm": "8.10.0"
  }
}
